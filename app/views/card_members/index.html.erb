<p id="notice"><%= notice %></p>

<h1>Listing Card Members</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @card_members.each do |card_member| %>
      <tr>
        <td><%= link_to 'Show', card_member %></td>
        <td><%= link_to 'Edit', edit_card_member_path(card_member) %></td>
        <td><%= link_to 'Destroy', card_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Card member', new_card_member_path %>
